{
  "name": "jakiro",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.0.0",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-thunk": "^2.2.0"
  },
  "lint-staged": {
    "{src,config}/**/*.{js,jsx,json,css,less,md}": "npm run lint:prettier -- --write",
    "src/**/*.{css,less}": "npm run lint:stylelint -- --fix"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "flow": "flow check",
    "precommit": "lint-staged && npm run flow",
    "prepush": "CI=true npm run test",
    "lint:css": "npm run lint:stylelint 'src/**/*.less'",
    "lint:js": "npm run lint:prettier 'src/**/*.js'",
    "lint:stylelint": "stylelint --config config/.stylelintrc",
    "lint:prettier": "prettier --config config/.prettierrc --ignore-path config/.prettierignore",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.5.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-shallow-until": "^1.0.1",
    "enzyme-to-json": "^3.2.2",
    "flow-bin": "^0.59.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "prettier": "^1.7.0",
    "react-app-rewire-less": "^2.1.0",
    "react-app-rewired": "^1.3.5",
    "react-scripts": "^1.0.17",
    "react-test-renderer": "^16.2.0",
    "redux-devtools-extension": "^2.13.2",
    "source-map-explorer": "^1.5.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
